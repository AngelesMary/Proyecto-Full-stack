             <div class="comment">
             <p v-for="items in item" v-text="items"></p>
             </div><!--End Comment-->
             </div><!--End col -->
             </div><!-- End row -->
             <div class="row"
             <div class="col-6">
             <textarea type="text" class="input" placeholder="Escribe tu comentario" v-model="newItem" @keyup.enter="addItem()"></textarea>
             <button v-on:click="addItem()" class='primaryContained float-right' type="submit">Comentar</button>
             </div><!-- End col -->
             </div><!--End Row -->
             </div><!--End Container -->
             </section><!-- end App -->
   
             </article>
